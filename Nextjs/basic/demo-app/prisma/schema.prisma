generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model students {
  ID    Int    @id @default(autoincrement())
  Name  String @db.VarChar(100)
  Email String @db.VarChar(100)
  Age   Int
}
